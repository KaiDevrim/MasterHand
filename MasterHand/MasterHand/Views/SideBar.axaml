<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MasterHand.Views.SideBar">
    <DockPanel>
        <ListBox SelectionChanged = "ChangePage" Width="150" DockPanel.Dock="Left" Background="LightGray">
            <ListBoxItem>Home</ListBoxItem>
            <ListBoxItem>Characters</ListBoxItem>
            <ListBoxItem>Stages</ListBoxItem>
            <ListBoxItem>Settings</ListBoxItem>
        </ListBox>
    </DockPanel>
</UserControl>
